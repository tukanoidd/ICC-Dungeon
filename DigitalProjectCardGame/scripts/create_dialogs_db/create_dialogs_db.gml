global.dialogs_db = ds_map_create();
ds_map_add(global.dialogs_db, 0, "You, of all people, made it so far? Ha! I have waited long enough. You are to entertain me, weary Challenger. Give it your best shot!");
ds_map_add(global.dialogs_db, 1, "There, there. Nice one.");
ds_map_add(global.dialogs_db, 2, "Ha! I see some slight hope that you may be different than others.");
ds_map_add(global.dialogs_db, 3, "Oh, yes. You are definitely worthy of making it this far.");
ds_map_add(global.dialogs_db, 4, "Finally, the ray of hope is drawing nearer. Do it, Challenger.");
ds_map_add(global.dialogs_db, 5, "So close, so close. A tad more, come on.");
ds_map_add(global.dialogs_db, 6, "YES! You have done it! Completing all the questions has been previously unheard of. Good job, master.");